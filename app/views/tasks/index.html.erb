<h1>To Do</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>
<%= render @to_do %>
  <tbody>
    <% @to_do.each do |task| %>
  
    <% end %>
  </tbody>
</table>

<br>


<h1>Doing</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>State</th>
      <th colspan="5"></th>
    </tr>
  </thead>
<%= render @doing %>
  <tbody>
    <% @doing.each do |task| %>
   
    <% end %>
  </tbody>
</table>

<br>



<h1>Done</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>State</th>
      <th colspan="5"></th>
    </tr>
  </thead>

  <tbody>
    <% @done.each do |task| %>
    <%= render @done %>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Task', new_task_path %>
